   "source": [
    "#인치(inch)를 센티미터(cm)으로 변환하는 프로그램을 작성하십시오.\n",
    "#이 때 1 인치는 2.54 센티미터입니다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "285149a8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "인치를 입력하세요:1\n",
      "1.00 inch =>  2.54 cm\n"
     ]
    }
   ],
   "source": [
    "def output(i):\n",
    "    result = i*2.54\n",
    "    return(print(f\"{i:.2f} inch =>  {result:.2f} cm\"))\n",
    "\n",
    "i = float(input(\"인치를 입력하세요:\"))\n",
    "output(i)"
   ]
  }
