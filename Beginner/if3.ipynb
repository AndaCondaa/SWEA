   "id": "82e8a93d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "e\n",
      "e 는 소문자 입니다.\n"
     ]
    }
   ],
   "source": [
    "def output(E):\n",
    "    if E.isupper():\n",
    "        print(f\"{E} 는 대문자 입니다.\")\n",
    "    else:\n",
    "        print(f\"{E} 는 소문자 입니다.\")\n",
    "\n",
    "E = input()\n",
    "output(E)"
   ]
  }
