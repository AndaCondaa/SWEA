   "outputs": [],
   "source": [
    "# 다음의 결과와 같이 임의의 양의 정수를 입력받아 그 정수의 모든 약수를 구하십시오"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "a6ced3a5",
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "def output(N):\n",
    "    for i in range(1,N+1):\n",
    "        if (N % i) == 0:\n",
    "            print(f\"{i}(은)는 {N}의 약수입니다.\")\n",
    "\n",
    "N = int(input())\n",
    "output(N)"
   ]
  }
 ],
