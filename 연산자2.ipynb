   "source": [
    "#킬로그램(kg)를 파운드(lb)으로 변환하는 프로그램을 작성하십시오.\n",
    "#이 때 1 킬로그램은 2.2046 파운드입니다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "52625dc9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "90\n",
      "90.00 kg =>  198.41 lb\n"
     ]
    }
   ],
   "source": [
    "def output(k):\n",
    "    result = k * 2.2046\n",
    "    return(print(f\"{k:.2f} kg =>  {result:.2f} lb\"))\n",
    "\n",
    "k = float(input())\n",
    "output(k)"
   ]
  },
