    "def output(N):\n",
    "    for i in range(1,N+1):\n",
    "        if (N % i) == 0:\n",
    "            print(f\"{i}(은)는 {N}의 약수입니다.\")\n",
    "\n",
    "N = int(input())\n",
    "output(N)"
